generator client {
  provider     = "prisma-client"
  output       = "../src/generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "mysql"
}

model Customers {
  id        Int    @id() @default(autoincrement())
  firstName String
  lastName  String
  email     String @unique()
  mobile    String @unique()
  city      String
  address   String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
